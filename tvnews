<div id="CBS" class="left">
CBS  (<a href="https://cbsnewshd-lh.akamaihd.net/i/CBSNHD_7@199302/master.m3u8">Direct</a>, <a href="https://video-dev.github.io/hls.js/demo/?src=https://cbsnewshd-lh.akamaihd.net/i/CBSNHD_7@199302/master.m3u8">hls.js</a>)
<video controls muted autoplay width="640" height="360" id="cbs1"></video>
<script>
  if(Hls.isSupported()) {
    var video = document.getElementById('cbs1');
    var hls = new Hls();
    hls.loadSource('https://cbsnewshd-lh.akamaihd.net/i/CBSNHD_7@199302/master.m3u8');
    hls.attachMedia(cbs1);
    hls.on(Hls.Events.MANIFEST_PARSED,function() {
      video.play();
  });
 }
</script>
</div>


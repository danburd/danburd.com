<script>
  if(Hls.isSupported()) {
    var video = document.getElementById('12');
    var hls = new Hls();
    hls.loadSource('https://abclive-lh.akamaihd.net/i/abc_live1@136327/master.m3u8');
    hls.attachMedia(12);
    hls.on(Hls.Events.MANIFEST_PARSED,function() {
      video.play();
  });
 }
</script>
</div>

